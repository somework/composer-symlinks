{
  "name": "somework/composer-symlinks",
  "type": "composer-plugin",
  "description": "Create relative symlinks based on getcwd() and relative data",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Pinchuk Igor",
      "email": "i.pinchuk.personal@gmail.com"
    },
    {
      "name": "Panychev Dmitry",
      "email": "thor_work@yahoo.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "SomeWork\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SomeWork\\Symlinks\\Tests\\": "tests/"
    }
  },
  "extra": {
    "class": "SomeWork\\Symlinks\\Plugin"
  },
  "require": {
    "php": ">=8.0",
    "composer-plugin-api": "^2.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "@stable",
    "composer/composer": "^2.0",
    "phpunit/phpunit": "^9"
  },
  "scripts": {
    "test": "phpunit"
  }
}
